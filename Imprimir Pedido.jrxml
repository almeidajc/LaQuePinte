	<queryString language="SQL">
		<![CDATA[select pedidos.`id_pedido`, pedidos.`fecha_entrega`, pedidos.`id_estado`, estado.`descripcion`
from pedidos
inner join estado
on pedidos.`id_estado` = estado.`id_estado`
where estado.`id_estado`=1 and pedidos.`fecha_entrega`>CURRENT_DATE() and pedidos.nombre='fede']]>
